<div class="project-teaser card col s12 m4 l3">
  <div class="card-content">
    <span class="title card-title"><%= link_to project.title, project_path(project)%> </span>
    (<p>Location: <%= project.creator.city %>, <%= project.creator.state %></p>)
    <p><%= project.description %></p>
    <p>Project Lead: <%= project.creator.username %></p>
 <div class="tags">
      <% project.tags.each do |tag| %>
        <div class="tag"><%= link_to tag.name, tag_path(tag), class: "tag"%></div>
  <% end %>
</div>

  </div>
</div>