<ul>
<% for user in @users %>
  <% for project in @users.projects_created %>
  <%= render partial: 'project', locals: { project: project} %>
  <% end %>
<% end %>
</ul>
<p>
  <strong>Name:</strong>
  <%= user.username %>
</p>
<p>
  <strong>First Name</strong>
  <%= user.first_name %>
</p>
<p>
  <strong>Last Name</strong>
  <%= user.last_name %>
</p>
<ul>
  <% @users.each do |user| %>
  <% user.nearbys(100).each do |near_user| %>
  <li>Distance: <%= near_user.distance.round(2) %></li>
  <% end %>
  <% end %>
</ul>
